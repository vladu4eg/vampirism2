<root>
    <styles>
 		<include src="s2r://panorama/styles/dotastyles.css" />
        <include src="file://{resources}/styles/custom_game/multiteam_end_screen.css" />
    </styles>
	<Panel class="PlayerRow">
		<Panel id="PlayerRowContainer">

			<Panel id="Hero" class="ScoreCol_Hero ScoreboardPanel">
				<Image id="HeroIconEnd" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect" />
			</Panel>

			<Panel id="PlayerDetailsContainer" class="ScoreCol_PlayerDetails ScoreboardPanel">
				<Panel id="PlayerAndHeroName">
					<Label id="PlayerName" text="AAAAA" />
				</Panel>
			</Panel>

			<Panel id="KillsContainer" class="ScoreCol_Kills ScoreboardPanel">
				<Label id="Kills" text="999" />
			</Panel>
			<Panel id="DeathsContainer" class="ScoreCol_Deaths ScoreboardPanel">
				<Label id="Deaths" text="999" />
			</Panel>
			<Panel id="GoldContainer" class="ScoreCol_Gold ScoreboardPanel">
				<Label id="PlayerGoldAmount" text="999" />
			</Panel>
			<Panel id="LumberContainer" class="ScoreCol_Gold ScoreboardPanel">
				<Label id="PlayerLumberAmount" text="999" />
			</Panel>
			<Panel id="GPSContainer" class="ScoreCol_Gold ScoreboardPanel">
				<Label id="PlayerGPSAmount" text="999" />
			</Panel>
			<Panel id="LPSContainer" class="ScoreCol_Gold ScoreboardPanel">
				<Label id="PlayerLPSAmount" text="999" />
			</Panel>
			<Panel id="GoldGivenContainer" class="ScoreCol_Gold ScoreboardPanel">
				<Label id="PlayerGoldGivenAmount" text="999" />
			</Panel>
			<Panel id="LumberGivenContainer" class="ScoreCol_lumber ScoreboardPanel">
				<Label id="PlayerLumberGivenAmount" text="999" />
			</Panel>
			<Panel id="ScoreContainer" class="ScoreCol_Score ScoreboardPanel">
				<Label id="PlayerScore" text="0" />
			</Panel>
			<Panel id="ChangeScoreContainer" class="ScoreCol_ChangeScore ScoreboardPanel">
				<Label id="PlayerChangeScore" text="0" />
			</Panel>
		</Panel>
	</Panel>
</root>
